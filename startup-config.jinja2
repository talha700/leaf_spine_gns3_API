service timestamps debug datetime msec
service timestamps log datetime msec
!
hostname Router
!
boot-start-marker
boot-end-marker
!
!
!
no aaa new-model
!
!
!
ip audit notify log
ip audit po max-events 100

ip cef
no ipv6 cef
!
!

!
multilink bundle-name authenticated
!
!
!
spanning-tree mode pvst
spanning-tree extend system-id
!
vlan internal allocation policy ascending
!
!
!
interface Ethernet0/0
 no ip address
 switchport
 switchport mode access
 switchport access vlan 1
 no shutdown
 duplex full
!
interface Ethernet0/1
 no ip address
 switchport
 switchport mode access
 switchport access vlan 1
 no shutdown
 duplex full
!
interface Ethernet0/2
 no ip address
 switchport
 switchport mode access
 switchport access vlan 1
 no shutdown
 duplex full
!
interface Ethernet0/3
 no ip address
 switchport
 switchport mode access
 switchport access vlan 1
 no shutdown
 duplex full
!
interface Ethernet1/0
 no ip address
 switchport
 switchport mode access
 switchport access vlan 1
 no shutdown
 duplex full
!
interface Ethernet1/1
 no ip address
 switchport
 switchport mode access
 switchport access vlan 1
 no shutdown
 duplex full
!
interface Ethernet1/2
 no ip address
 switchport
 switchport mode access
 switchport access vlan 1
 no shutdown
 duplex full
!
interface Ethernet1/3
 no ip address
 switchport
 switchport mode access
 switchport access vlan 1
 no shutdown
 duplex full
!
interface Vlan1
 ip address {{ ip }} 255.255.255.0
!
ip forward-protocol nd
!
!
no ip http server
no ip http secure-server
!
!
control-plane
!
!
line con 0
 logging synchronous
line aux 0
line vty 0 4
 login
!
end